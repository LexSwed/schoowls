datasource db {
    provider = "sqlite"
    url = "***"
}

generator client {
    provider = "prisma-client-js"
}

model Account {
    id             Int      @default(autoincrement()) @id
    user           User
    active         Boolean
    registeredAt   DateTime
    lastLoggedInAt DateTime
}

model User {
    id        Int     @default(autoincrement()) @id
    account   Account @relation(fields: [accountId], references: [id])
    accountId Int
}
